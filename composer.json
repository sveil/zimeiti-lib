{
  "name": "sveil/zimeiti-lib",
  "description": "Development library of Zimeiti Contest Management System for Sveil",
  "keywords": [
    "sveil",
    "zimeiti",
    "library"
  ],
  "homepage": "http://sveil.com",
  "type": "library",
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Richard",
      "email": "richard@sveil.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-openssl": "*",
    "qiniu/php-sdk": "^7.2",
    "aliyuncs/oss-sdk-php": "^2.3"
  },
  "autoload": {
    "files": [
      "lang/en-us.php",
      "lang/zh-cn.php",
      "common.php",
      "sys.php"
    ],
    "psr-4": {
      "sveil\\": "src"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer"
    }
  },
  "config": {
    "process-timeout": 1200
  }
}
